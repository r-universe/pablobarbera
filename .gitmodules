[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "instaR"]
	path = instaR
	url = https://github.com/pablobarbera/instar
	shallow = true
	subdir = instaR
	registered = true
[submodule "Rfacebook"]
	path = Rfacebook
	url = https://github.com/pablobarbera/rfacebook
	shallow = true
	subdir = Rfacebook
	registered = true
[submodule "ROAuth"]
	path = ROAuth
	url = https://github.com/pablobarbera/roauth
	shallow = true
	registered = true
[submodule "streamR"]
	path = streamR
	url = https://github.com/cran/streamR
	shallow = true
	registered = true
